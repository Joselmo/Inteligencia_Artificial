	</category>

	<!-- regla 42 -->
	<category>
		<pattern>^ OFICINA * PROFESOR DE PRACTICAS</pattern>
		<template>despacho nº 24 de la 4º planta.</template>
	</category>

	<!-- regla 43 -->
	<category>
		<pattern>^ OFICINA * PROFESOR DE TEORIA</pattern>
		<template>despacho nº 23 de la 4º planta.</template>
	</category>	

	<!-- regla 42.1 -->
	<category>
		<pattern>^ DONDE * DESPACHO DEL PROFESOR DE *</pattern>
		<template>
			<think>
				<set var="tipo"><star index="3"/></set>
			</think>
			<condition var="tipo">
				<li value="teoria">En el <srai>^ OFICINA * PROFESOR DE TEORIA</srai></li>
				<li value="practicas">En el <srai>^ OFICINA * PROFESOR DE PRACTICAS</srai></li>
			</condition>
		</template>
	</category>

	<!-- regla 44 -->
	<category>
